

@model CE.ViewModels.ModelViewModel


@using Microsoft.AspNetCore.Identity
@using CE.Data

@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

@{
    ViewData["Title"] = "Index";
}
<section class="content-header">
    <h1>
        Models
        <small>Model</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Models</a></li>
        <li class="active">Model</li>
    </ol>
</section>
<hr />
<div>
    <form class="container" asp-action="Searchvisitdaily" method="post">

        <div class="row">
            <div class="col-md-6">
                <input type="date" name="startDate" class="form-control" placeholder="Start date">
            </div>
            <div class="col-md-6">
                <input type="date" name="endDate" class="form-control" placeholder="End date">
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <input type="text" name="searchvisitdaily" class="form-control" placeholder="Search...">
            </div>
            <div class="col-md-6">
                <a asp-action="AddOrEditModels" method="post">
                    @Html.DropDownListFor(x => Model.SelectedModel, Html.GetEnumSelectList<CE.Data.ModelEnum>(), htmlAttributes: new { @class = "form-control", id = "Models" })
                    
                </a>
            </div>
        </div>
        <div class="row">
            <button type="submit" name="searchvisitdaily" id="search-btn" class="btn btn-info btn-flat">
                <i class="fa fa-search"></i> Search
            </button>
        </div>
    </form>
</div>