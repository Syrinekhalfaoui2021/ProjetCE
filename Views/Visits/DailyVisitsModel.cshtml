@model IEnumerable<CE.Data.Visits>
@using Microsoft.AspNetCore.Identity
@using CE.Data

@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

@{
    ViewData["Title"] = "Index";
}

<section class="content-header">
    <h1>
        Visits
        <small>Visit daily </small>
        <small>Visit daily Models </small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#">Visits</a></li>
        <li class="active">Visit daily</li>
        <li class="active">Visit daily Models</li>
    </ol>
</section>
<hr />
<table class="table table-hover">
    <thead>
        <tr>

            <th>

                Brand
            </th>
            <th>
                Category
            </th>

            <th>
                Model
            </th>
            <th>
                Shelf share
            </th>
            <th>
                W/S
            </th>
            <th>
                Price
            </th>

            <th>
                <a asp-action="Indexe" class="btn btn-block btn-primary">SAVE</a>

            </th>



        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Brand.Namebrand)
                </td>

                <td>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Models.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Models.ShelfShare)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Models.MarketShare)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Models.Price)
                </td>

                <td>
                </td>

                <td>



                </td>

            </tr>
        }
    </tbody>
</table>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}